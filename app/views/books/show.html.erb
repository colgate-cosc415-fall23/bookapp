<section>
<div class="container">
  <h1>Details for <%= @book.title %></h1>
  <dl>
  <dt>Publisher</dt>
  <dd><%= @book.publisher %></dd>
  <dt>Year</dt>
  <dd><%= @book.year %></dd>
  <dt>Pages</dt>
  <dd><%= @book.pages %></dd>
  <dt>Price</dt>
  <dd><%= @book.list_price %></dd>
  </dl> 
  <br>

  <%= link_to "Back to index", books_path, class: "btn btn-primary" %>
<div>
</section>
